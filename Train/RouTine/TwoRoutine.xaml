            <Style TargetType="StackPanel">
                <EventSetter Event="MouseDown" Handler="StackPanel_MouseDown" />
                <Setter Property="Cursor" Value="Arrow" />
            </Style>
            <Style TargetType="Ellipse">
                <Setter Property="Fill">
                    <Setter.Value>
                        <RadialGradientBrush>
                            <GradientStop Color="AntiqueWhite" Offset="0.975" />
                            <GradientStop Color="#0596" Offset="0.475"/>
                            <GradientStop Color="Azure" Offset="0.993" />
                        </RadialGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Cursor" Value="Hand" />
                <Setter Property="ForceCursor" Value="True" />
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Width" Value="39" />
                        <Setter Property="Height" Value="39" />
                        <Setter Property="Fill">
                            <Setter.Value>
                                <RadialGradientBrush>
                                    <GradientStop Color="AntiqueWhite" Offset="0.975" />
                                    <GradientStop Color="#8999" Offset="0.475" />
                                    <GradientStop Color="Azure" Offset="0.993" />
                                </RadialGradientBrush>
                            </Setter.Value>
                        </Setter>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="TextBlock">
                <Setter Property="FontFamily" Value="黑体"/>
                <Setter Property="Foreground" Value="White"/>
                <Setter Property="FontSize" Value="20"/>
            </Style>
            <Style TargetType="Label">
                <Setter Property="Foreground" Value="WhiteSmoke"/>
                <Setter Property="FontFamily" Value="黑体"/>
            </Style>
