<Window x:Class="Train.Rou"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="购票" Height="520" Width="940">
    <Window.Background>
        <ImageBrush ImageSource="/image/9.jpg" />
    </Window.Background>
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Xmlresource/Language.xaml" />
                <ResourceDictionary Source="/Xmlresource/Zh-Cn.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid DataContext="">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="65*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Button Content="返回首页" Grid.Column="1" HorizontalAlignment="Left" Margin="345,11,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2" Height="22"/>
        <Frame Name="f1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" NavigationUIVisibility  ="Hidden" Grid.Row="1" Grid.ColumnSpan="2"/>
        <Label Content="{DynamicResource Subway}" HorizontalAlignment="Left" Margin="44,10,0,0" VerticalAlignment="Top" FontSize="17"/>
    </Grid>
</Window>
