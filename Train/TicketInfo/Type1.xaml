<Page x:Class="Train.TicketOver.Type1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="Type1">
    <Grid>
        <Label Name="mess" Content="票价(或通票)：" Margin="30,90,148,0" FontSize="17" VerticalContentAlignment="Center" Height="30" VerticalAlignment="Top"/>
        <TextBox Name="numl" IsEnabled="False" Margin="157,90,0,0" Width="96" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" VerticalContentAlignment="Center" TextChanged="numl_TextChanged"/>
        <RadioButton FontSize="20" Name="r1" Content="单程票" Foreground="Maroon" Grid.Column="1" HorizontalAlignment="Left" Margin="30,140,0,0" VerticalAlignment="Top" Checked="r1_Checked"/>
        <RadioButton  FontSize="20" Name="r2" Content="双程票"  Foreground="Maroon" HorizontalAlignment="Left" Margin="166,140,0,0" VerticalAlignment="Top" Checked="r1_Checked"/>
    </Grid>
</Page>
